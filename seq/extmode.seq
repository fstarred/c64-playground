;***************************************;; EXTENDED MODE;; SET 2 MOST SIGNIFICANT BIT; OF CHARACTER CODE (I.E.$400-$7E8); FOR DISPLAYING  DIFFERENT BACKGROUND;;;***************************************      *= $1000      SEI      JSR $E544      LDX #0      LDA #6LOOPCOL      STA $D800,X      INX      BNE LOOPCOL      LDA #0      STA $D021      LDA #1      STA $D022      LDA #2      STA $D023      LDA #3      STA $D024      LDA $D011      ORA #$40      STA $D011; EXTEND MODE      LDX #0      LDA MSGLOOP      STA $0400,X      INX      LDA MSG,X      BNE LOOP; WRITE MSG      LDX #3LOOP1      LDA $0403,X      EOR #$40; CLEAR BIT #6      STA $0403,X      DEX      BNE LOOP1      LDX #3LOOP2      LDA $040B,X      EOR #$40; CLEAR BIT #6      STA $040B,X      DEX      BNE LOOP2      JMP *MSG      .TEXT "ABC ABC ÁÂÃ ÁÂÃ"      .BYTE 0; FINAL BYTE OUTPUT WOULD BE:; $41,$42,$43,$01,$02,$03; $C1,$C2,$C3,$81,$82,$83