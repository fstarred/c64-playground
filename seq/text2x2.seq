;****************************;; PRINT TEXT USING FONT2X2;; CORRECT FONT POSITION MAY BE FOUND;; USING FOLLOWING FORMULA:;; (CHAR BYTE VALUE - 40) * 4;;****************************         *= $1000         JSR $E544         ; READ CHAR FROM $2000         LDA #$18         STA $D018         ; MULTICOLOR MODE         LDA #$D8         STA $D016         ; BG COLOR         LDA #$00         STA $D021         ; BG COLOR 1         LDA #$06         STA $D022         ; BG COLOR 2         LDA #$01         STA $D023         LDX #0CLEARRAM         LDA #0         STA $0400,X         STA $0500,X         STA $0600,X         STA $06E8,X         LDA #11         STA $D800,X         STA $D900,X         STA $DA00,X         STA $DAE8,X         INX         BNE CLEARRAM         LDA MSG         LDY #0         LDX #0PRINTMSG         SEC         SBC #$40         ASL A         ASL A         CLC         STA $0400,X         ADC #1         STA $0401,X         ADC #1         STA $0400+40,X         ADC #1         STA $0401+40,X         INX         INX         INY         LDA MSG,Y         BNE PRINTMSG         JMP *MSG         .TEXT "STARRED-MEDIASOFT"         .BYTE 0         ; LOAD CHROME.64C AT $2000