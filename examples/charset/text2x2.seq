;---------------------------------------;;†PRINT TEXT USING FONT2X2;; CORRECT FONT POSITION MAY BE FOUND;; USING FOLLOWING FORMULA:;; (CHAR BYTE VALUE - 40) * 4;;---------------------------------------;;             “≈¡ƒÕ≈;; LOAD CHROME.64C ON ADDRESS†$2000;;;---------------------------------------         *= $1000         JSR $E544         ; READ CHAR FROM $2000         LDA #$18         STA $D018         ; MULTICOLOR MODE         LDA #$D8         STA $D016         ; BG COLOR         LDA #$00         STA $D021         ; BG COLOR 1         LDA #$06         STA $D022         ; BG COLOR 2         LDA #$01         STA $D023         LDX #0CLEARRAM         LDA #0         STA $0400,X         STA $0500,X         STA $0600,X         STA $06E8,X         LDA #11         STA $D800,X         STA $D900,X         STA $DA00,X         STA $DAE8,X         INX         BNE CLEARRAM         LDA MSG         LDY #0         LDX #0PRINTMSG         EOR #$40         ASL A         ASL A         STA $040B,X         ADC #1         STA $040C,X         ADC #1         STA $040B+40,X         ADC #1         STA $040C+40,X         INX         INX         INY         LDA MSG,Y         BNE PRINTMSG         LDA MSG2         LDY #0         LDX #0         CLCPRINTMSG2         EOR #$40         ASL A         ASL A         STA $0459,X         ADC #1         STA $045A,X         ADC #1         STA $0459+40,X         ADC #1         STA $045A+40,X         INX         INX         INY         LDA MSG2,Y         BNE PRINTMSG2         JMP *MSG         .TEXT "STARRED"         .BYTE 0MSG2         .TEXT "MEDIASOFT"         .BYTE 0         ; LOAD CHROME.64C ON $2000